{
  "name": "onechat-core",
  "version": "1.0.0",
  "description": "Backend express server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint . --fix",
    "watch": "tsc -w --pretty",
    "start": "node .",
    "build": "rimraf dist/ && tsc",
    "prestart": "prisma db push --skip-generate",
    "pretty": "prettier --write \"src/**/*.{js,ts,json,yml}\"",
    "gen-env": "gen-env-types .env -o src/typings/env.d.ts -e .",
    "postinstall": "prisma generate"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "keywords": [
    "ExpressJS",
    "Typescript"
  ],
  "license": "AGPL-3.0",
  "private": true,
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv-safe": "^8.1.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.13",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "gen-env-types": "^1.3.4",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "prisma": "^5.9.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
