    "Typescript"
  ],
  "license": "AGPL-3.0",
  "private": true,
  "dependencies": {
    "bcrypt": "^5.1.0",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "ioredis": "^5.3.1",
    "mongodb": "^5.1.0",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.20",
    "@types/cors": "^2.8.13",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "gen-env-types": "^1.3.4",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  }
