{
  "name": "onechat-core",
  "version": "0.0.1",
  "description": "Backend express server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint . --fix",
    "watch": "tsc -w --pretty",
    "start": "node .",
    "build": "rimraf dist/ && tsc",
    "gen-env": "gen-env-types .env -o src/typings/env.d.ts -e ."
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "keywords": [
