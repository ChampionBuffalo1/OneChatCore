{
  "name": "onechat-core",
  "version": "0.0.1",
  "description": "Backend express server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint . --fix",
    "watch": "tsc -w --pretty",
    "start": "node .",
    "build": "rimraf dist/ && tsc",
    "pretty": "prettier --write \"src/**/*.{js,ts,json,yml}\"",
    "gen-env": "gen-env-types .env -o src/typings/env.d.ts -e ."
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "keywords": [
    "ExpressJS",
    "Typescript"
  ],
  "license": "AGPL-3.0",
  "private": true,
  "dependencies": {
    "@prisma/client": "^4.16.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.6.0",
    "winston": "^3.9.0",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.6.0",
    "gen-env-types": "^1.3.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.7",
    "prisma": "^4.16.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.5"
  }
}
